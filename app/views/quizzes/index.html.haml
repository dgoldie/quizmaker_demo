%h1 Listing quizzes

%table
  %tr

- for quiz in @quizzes
  %tr
    %td=h quiz.name
    %td= link_to 'Show', quiz_path(quiz)
    %td= link_to 'Edit', edit_quiz_path(quiz)
    %td= link_to 'Destroy', quiz_path(quiz), :confirm => 'Are you sure?', :method => :delete, :confirm => "Are you sure?", :method => :delete
    
= link_to 'Add Quiz', new_quiz_path