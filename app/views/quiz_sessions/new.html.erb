<h1>New quiz_session</h1>

<% form_for(@quiz_session) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= label :quiz_session, :user, 'Select User' %>
    <%= collection_select(:quiz_session, :user_id, User.all, :id, :username) %>
  </p>
  <p>
    <%= label :quiz_session, :quiz, 'Select Quiz' %>
    <%= collection_select(:quiz_session, :quiz_id, Quiz.all, :id, :name) %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', quiz_sessions_path %>